    "@babel/preset-env": "^7.10.2",
    "apollo-server": "^2.14.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "fabric-ca-client": "2.1.2",
    "fabric-network": "2.1.2",
    "graphql": "^15.1.0",
    "graphql-scalars": "^1.1.5",
    "ioredis": "^4.17.3",
    "ipfs": "^0.47.0",
    "jsonwebtoken": "^8.5.1",
